    {
      "cwd": "${workspaceRoot}",
      "executable": "${workspaceFolder}/build/app/gd32f3-eval.elf",
      "name": "Debug with OpenOCD",
      "request": "launch",
      "type": "cortex-debug",
      "servertype": "openocd",
      "svdFile": "${workspaceFolder}/debug/GD32F30x_HD.svd",
      "configFiles": [
        "interface/stlink-v2.cfg",
        "target/stm32f3x.cfg"
      ],
      "gdbTarget": ":3333",
      "gdbPath": "gdb",
      "searchDir": [],
      "runToEntryPoint": "main",
      "showDevDebugOutput": "none",
      "preLaunchTask": "Build + Flash",
      "liveWatch": {
        "enabled": true,
        "samplesPerSecond": 4
      }
    },
